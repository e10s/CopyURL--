{
	"manifest_version": 2,
	"name": "Copy URL--",
	"description": "Copy URL.",
	"version": "0.1.1",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"menus",
		"clipboardWrite"
	],
	"content_scripts": [
		{
			"matches": [
				"https://*/*"
			],
			"js": [
				"content.js"
			]
		}
	]
}